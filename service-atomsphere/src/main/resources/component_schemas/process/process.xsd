<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
           xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
           xmlns:ih="http://jaxb2-commons.dev.java.net/basic/inheritance"
           jxb:extensionBindingPrefixes="xjc ih"
           jxb:version="2.0">
    <xs:complexType name="Shape">
        <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element name="configuration">
                <xs:complexType>
                    <xs:sequence>
                        <xs:any>
                            <xs:annotation>
                                <xs:appinfo>
                                    <jxb:property name="value"/>
                                </xs:appinfo>
                            </xs:annotation>
                        </xs:any>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="dragpoints">
                <xs:annotation>
                    <xs:appinfo>
                        <jxb:property name="dragPoints"/>
                    </xs:appinfo>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:appinfo>
                                <jxb:property name="list"/>
                            </xs:appinfo>
                        </xs:annotation>
                        <xs:element name="dragpoint" type="DragPoint"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="notes" type="xs:string" />
        </xs:sequence>

        <xs:attribute name="name" type="xs:string"/>
        <xs:attribute name="userlabel" type="xs:string"/>
        <xs:attribute name="image" type="xs:string"/>
        <xs:attribute name="x" type="xs:float"/>
        <xs:attribute name="y" type="xs:float"/>
        <xs:attribute name="shapetype" type="ShapeType">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property name="shapeType"/>
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="DragPoint">
        <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property name="lineSegments"/>
                </xs:appinfo>
            </xs:annotation>
            <xs:element name="linesegment" type="LineSegment"/>
        </xs:sequence>

        <xs:attribute name="name" type="xs:string"/>
        <xs:attribute name="fromShape" type="xs:string"/>
        <xs:attribute name="toShape" type="xs:string"/>
        <xs:attribute name="identifier" type="xs:string"/>
        <xs:attribute name="text" type="xs:string"/>
        <xs:attribute name="x" type="xs:float"/>
        <xs:attribute name="y" type="xs:float"/>
        <xs:attribute name="lastX" type="xs:float"/>
        <xs:attribute name="lastY" type="xs:float"/>
    </xs:complexType>

    <xs:complexType name="LineSegment">
        <xs:attribute name="name" type="xs:string"/>
        <xs:attribute name="length" type="xs:int"/>
        <xs:attribute name="orient">
            <xs:simpleType>
                <xs:annotation>
                    <xs:appinfo>
                        <jxb:typesafeEnumClass name="Orientation"/>
                    </xs:appinfo>
                </xs:annotation>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="horizontal"/>
                    <xs:enumeration value="vertical"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="label" type="xs:string"/>
        <xs:attribute name="x" type="xs:float"/>
        <xs:attribute name="y" type="xs:float"/>
    </xs:complexType>

    <xs:simpleType name="ShapeType">
        <xs:annotation>
            <xs:appinfo>
                <typesafeEnumClass map="false">
                </typesafeEnumClass>
            </xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="branch"/>
            <xs:enumeration value="route"/>
            <xs:enumeration value="documentproperties"/>
            <xs:enumeration value="connectoraction"/>
            <xs:enumeration value="tradingpartneraction"/>
            <xs:enumeration value="merge"/>
            <xs:enumeration value="start"/>
            <xs:enumeration value="returndocuments"/>
            <xs:enumeration value="flowcontrol"/>
            <xs:enumeration value="decision"/>
            <xs:enumeration value="exception"/>
            <xs:enumeration value="notify"/>
            <xs:enumeration value="message"/>
            <xs:enumeration value="map"/>
            <xs:enumeration value="note"/>
            <xs:enumeration value="programexecution"/>
            <xs:enumeration value="programcmd"/>
            <xs:enumeration value="stop"/>
            <xs:enumeration value="processcall"/>
            <xs:enumeration value="dataprocess"/>
            <xs:enumeration value="cleanse"/>
            <xs:enumeration value="changedatacapture"/>
            <xs:enumeration value="businessrules"/>
            <xs:enumeration value="catcherrors"/>
            <xs:enumeration value="doccacheload"/>
            <xs:enumeration value="doccacheretrieve"/>
            <xs:enumeration value="processroute"/>
            <xs:enumeration value="doccacheremove"/>
            <!-- <xs:enumeration value="documentsplit"/>-->
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="process">
        <xs:complexType>
            <xs:annotation>
                <xs:appinfo>
                    <ih:implements>com.boomi.model.common.Copyable</ih:implements>
                </xs:appinfo>
            </xs:annotation>
            <xs:sequence>
                <xs:element name="shapes">
                    <xs:complexType>
                        <xs:sequence minOccurs="1" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:appinfo>
                                    <jxb:property name="list"/>
                                </xs:appinfo>
                            </xs:annotation>
                            <xs:element name="shape" type="Shape"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="schedules">
                    <xs:complexType>
                        <xs:sequence minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:appinfo>
                                    <jxb:property name="list"/>
                                </xs:appinfo>
                            </xs:annotation>
                            <xs:element name="schedule" type="Schedule"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="retrySchedules">
                    <xs:complexType>
                        <xs:sequence minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:appinfo>
                                    <jxb:property name="list"/>
                                </xs:appinfo>
                            </xs:annotation>
                            <xs:element name="schedule" type="Schedule"/>
                        </xs:sequence>
                        <xs:attribute name="maxRetries" type="xs:int" default="5"/>                        
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="allowSimultaneous" type="xs:boolean" default="true"/>
            <xs:attribute name="enableUserLog" type="xs:boolean" default="false"/>
            <xs:attribute name="updateRunDates" type="xs:boolean" default="true"/>
            <xs:attribute name="processLogOnErrorOnly" type="xs:boolean" default="false"/>
            <xs:attribute name="workload" type="ProcessWorkloadType"/>
            <xs:attribute name="purgeDataImmediately" type="xs:boolean" default="false"/>
        </xs:complexType>
    </xs:element>

    <xs:simpleType name="ProcessWorkloadType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="general"/>
            <xs:enumeration value="low_latency"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="ScheduleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="minute"/>
            <xs:enumeration value="hour"/>
            <xs:enumeration value="day"/>
            <xs:enumeration value="month"/>
            <xs:enumeration value="year"/>
            <xs:enumeration value="advanced"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="Schedule">
        <xs:attribute name="type" type="ScheduleType"/>
        <xs:attribute name="minute" type="xs:string"/>
        <xs:attribute name="hour" type="xs:string"/>
        <xs:attribute name="dayofweek" type="xs:string"/>
        <xs:attribute name="dayofmonth" type="xs:string"/>
        <xs:attribute name="month" type="xs:string"/>
        <xs:attribute name="year" type="xs:string"/>
        <xs:attribute name="name" type="xs:string"/>
    </xs:complexType>

    <!-- Shapes -->

    <!-- Common elements -->


    <xs:simpleType name="CompareType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="equals"/>
            <xs:enumeration value="notequals"/>
            <xs:enumeration value="greaterthan"/>
            <xs:enumeration value="greaterthaneq"/>
            <xs:enumeration value="lessthan"/>
            <xs:enumeration value="lessthaneq"/>
            <xs:enumeration value="regex"/>
            <xs:enumeration value="wildcard"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="parametervalue">
      <xs:annotation>
          <xs:appinfo>
              <jxb:class name="ParameterValue"/>
          </xs:appinfo>
      </xs:annotation>
      <xs:complexType>
        <xs:complexContent>
          <xs:extension base="Parameter"></xs:extension>
        </xs:complexContent>
      </xs:complexType>    
    </xs:element>

    <xs:simpleType name="parameterdatatype">
        <xs:annotation>
            <xs:appinfo>
                <jxb:typesafeEnumClass name="ParameterDataType"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:short">
            <xs:enumeration value="1">
                <xs:annotation>
                    <xs:appinfo>
                        <jxb:typesafeEnumMember name="STRING"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
                <xs:annotation>
                    <xs:appinfo>
                        <jxb:typesafeEnumMember name="INT"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:appinfo>
                        <jxb:typesafeEnumMember name="FLOAT"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
                <xs:annotation>
                    <xs:appinfo>
                        <jxb:typesafeEnumMember name="DATETIME"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:complexType name="Parameter">
        <xs:sequence>
            <xs:any>
                <xs:annotation>
                    <xs:appinfo>
                        <jxb:property name="value"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:any>
        </xs:sequence>
        <xs:attribute name="name" type="xs:string"/>
        <xs:attribute name="valueType" type="ParameterType"/>
        <xs:attribute name="dataType" type="parameterdatatype"/>
        <xs:attribute name="key" type="xs:string"/>
        <xs:attribute name="childKey" type="xs:string"/>
        <xs:attribute name="elementToSetId" type="xs:int"/>
        <xs:attribute name="elementToSetName" type="xs:string"/>
        <xs:attribute name="tagListKey" type="xs:string"/>
        <xs:attribute name="tagListValueLabel" type="xs:string"/>
    </xs:complexType>




    <xs:simpleType name="ParameterType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="static"/>
            <xs:enumeration value="process"/>
            <xs:enumeration value="sql"/>
            <xs:enumeration value="sp"/>
            <xs:enumeration value="unique"/>
            <xs:enumeration value="track"/>
            <xs:enumeration value="profile"/>
            <xs:enumeration value="errormessage"/>
            <xs:enumeration value="execution"/>
            <xs:enumeration value="keygen"/>
            <xs:enumeration value="date"/>
            <xs:enumeration value="command"/>
            <xs:enumeration value="connector"/>
            <xs:enumeration value="current"/>
            <xs:enumeration value="crossref"/>
            <xs:enumeration value="tradingpartnerref"/>
            <xs:enumeration value="documentcache"/>
            <xs:enumeration value="definedparameter"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ProfileType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="profile.edi"/>
            <xs:enumeration value="profile.db"/>
            <xs:enumeration value="profile.flatfile"/>
            <xs:enumeration value="profile.xml"/>
            <xs:enumeration value="profile.json"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="staticparameter">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="StaticParameter"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="staticproperty" type="xs:string"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="processparameter">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="ProcessParameter"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="processproperty" type="xs:string"/>
            <xs:attribute name="processpropertydefaultvalue" type="xs:string"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="executionparameter">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="ExecutionParameter"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="executionproperty" type="executionPropertyType"/>
        </xs:complexType>
    </xs:element>

    <xs:simpleType name="executionPropertyType">
        <xs:restriction  base="xs:string">
            <xs:enumeration value="Account Id"/>
            <xs:enumeration value="Atom Id"/>
            <xs:enumeration value="Atom Name"/>
            <xs:enumeration value="Document Count"/>
            <xs:enumeration value="Execution Id"/>
            <xs:enumeration value="Node Id"/>
            <xs:enumeration value="Process Id"/>
            <xs:enumeration value="Process Name"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="dynamicdocumentparameter">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="DynamicDocumentParameter"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="dynamicdocumentproperty" type="xs:string"/>
            <xs:attribute name="dynamicdocumentpropertydefaultvalue" type="xs:string"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="dateparameter">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="DateParameter"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence minOccurs="0" maxOccurs="1">
                <xs:element name="datedelta" type="DateDelta"/>
            </xs:sequence>

            <xs:attribute name="datetimemask" type="xs:string"/>
            <xs:attribute name="dateparametertype" type="DateParameterType"/>
        </xs:complexType>
    </xs:element>

    <xs:simpleType name="DateParameterType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="current"/>
            <xs:enumeration value="relative"/>
            <xs:enumeration value="last"/>
            <xs:enumeration value="lastsuccessful"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="DateDelta">
        <xs:attribute name="sign" type="Sign"/>
        <xs:attribute name="value" type="xs:int"/>
        <xs:attribute name="unit" type="Unit"/>
    </xs:complexType>

    <xs:simpleType name="Sign">
        <xs:annotation>
            <xs:appinfo>
                <typesafeEnumClass map="false">
                </typesafeEnumClass>
            </xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="plus"/>
            <xs:enumeration value="minus"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="Unit">
        <xs:annotation>
            <xs:appinfo>
                <typesafeEnumClass map="false">
                </typesafeEnumClass>
            </xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="seconds"/>
            <xs:enumeration value="minutes"/>
            <xs:enumeration value="hours"/>
            <xs:enumeration value="days"/>
            <xs:enumeration value="weeks"/>
            <xs:enumeration value="months"/>
            <xs:enumeration value="years"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="keygenparameter">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="SequentialParameter"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="keyname" type="xs:string"/>
            <xs:attribute name="keyfixtolength" type="xs:int"/>
            <xs:attribute name="batchSize" type="xs:int" default="1"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="trackparameter">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="TrackParameter"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="propertyId" type="xs:string"/>
            <xs:attribute name="propertyName" type="xs:string"/>
            <xs:attribute name="defaultValue" type="xs:string"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="definedprocessparameter">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="DefinedProcessParameter"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="componentId" type="xs:string"/>
            <xs:attribute name="componentName" type="xs:string"/>
            <xs:attribute name="propertyKey" type="xs:string"/>
            <xs:attribute name="propertyLabel" type="xs:string"/>
            <xs:attribute name="propertyValue" type="xs:string"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="sqlparameter">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="SqlParameter"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="sqltoexecute" type="xs:string"/>
                <xs:element name="parameters">
                    <xs:complexType>
                        <xs:sequence minOccurs="0" maxOccurs="unbounded">
                            <xs:element name="parametervalue" type="Parameter"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="connection" type="xs:string"/>
            <xs:attribute name="outputpos" default="1" type="xs:int"/>
            <xs:attribute name="cachevalues" type="xs:boolean"/>
            <xs:attribute name="outputdatatype" type="parameterdatatype"/>
        </xs:complexType>
    </xs:element>


    <xs:element name="connectorparameter">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="ConnectorParameter"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="inputs">
                    <xs:complexType>
                        <xs:sequence minOccurs="0" maxOccurs="unbounded">
                            <xs:element name="parametervalue" type="Parameter"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="connectorType" type="xs:string"/>
            <xs:attribute name="actionType" type="xs:string"/>
            <xs:attribute name="operationId" type="xs:string"/>
            <xs:attribute name="connectionId" type="xs:string"/>
            <xs:attribute name="outputParamName" type="xs:string"/>
            <xs:attribute name="outputParamId" type="xs:string"/>
            <xs:attribute name="tagListKey" type="xs:string"/>
            <xs:attribute name="tagListValueLabel" type="xs:string"/>
            <xs:attribute name="enforceSingleResult" type="xs:boolean" default="false"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="crossrefparameter">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="CrossRefParameter"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="inputs">
                    <xs:complexType>
                        <xs:sequence minOccurs="0" maxOccurs="unbounded">
                            <xs:element name="parametervalue" type="Parameter"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="crossRefTableId" type="xs:string"/>
            <xs:attribute name="outputParamName" type="xs:string"/>
            <xs:attribute name="outputParamId" type="xs:int"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="tradingpartnerparameter">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="TradingPartnerParameter"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="partnerId" type="xs:string"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="spparameter">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="StoredProcedureParameter"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>

                <xs:element name="parameters">
                    <xs:complexType>
                        <xs:sequence minOccurs="0" maxOccurs="unbounded">
                            <xs:element name="parametervalue" type="Parameter"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>

            <xs:attribute name="sqltoexecute" type="xs:string"/>
            <xs:attribute name="connection" type="xs:string"/>
            <xs:attribute name="outputpos" type="xs:string"/>
            <xs:attribute name="cachevalues" type="xs:boolean"/>
            <xs:attribute name="outputdatatype" type="parameterdatatype"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="profileelement">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="ProfileElement"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="elementId" type="xs:string"/>
            <xs:attribute name="elementName" type="xs:string"/>
            <xs:attribute name="tagListKey" type="xs:string"/>
            <xs:attribute name="tagListValueLabel" type="xs:string"/>
            <xs:attribute name="profileId" type="xs:string"/>
            <xs:attribute name="profileType" type="ProfileType"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="commandparameter">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="CommandParameter"/>
            </xs:appinfo>
        </xs:annotation>

        <xs:complexType>
            <xs:attribute name="command" type="xs:string"/>
        </xs:complexType>
    </xs:element>

    <!-- Branch -->
    <xs:element name="branch">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="Branch"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="numBranches" type="xs:int"/>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="BaseProcessCall" abstract="true">
        <xs:sequence>
            <xs:element name="parameters">
                <xs:complexType>
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:appinfo>
                                <jxb:property name="parameterValues"/>
                            </xs:appinfo>
                        </xs:annotation>
                        <xs:element name="parametervalue" type="Parameter"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="returnpaths">
                <xs:complexType>
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:appinfo>
                                <jxb:property name="returnpaths"/>
                            </xs:appinfo>
                        </xs:annotation>
                        <xs:element name="returnpaths" type="ReturnPath"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="wait" type="xs:boolean"/>
        <xs:attribute name="abort" type="xs:boolean"/>
    </xs:complexType>

    <!-- ProcessCall-->
    <xs:element name="processcall">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="ProcessCall"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="BaseProcessCall">
                    <xs:attribute name="processId" type="xs:string"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="processroutecall">
        <xs:annotation>
            <xs:appinfo>
                <jxb:class name="ProcessRouteCall"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="BaseProcessCall">
                    <xs:sequence>
                        <xs:element name="routeParameter" minOccurs="0" maxOccurs="unbounded" type="Parameter">
                            <xs:annotation>
                                <xs:appinfo>
                                    <jxb:property name="routeParameters"/>
                                </xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="routeType" type="ProcessRouteType" default="processroute"/>
                    <xs:attribute name="processRouteId" type="xs:string"/>
                    <xs:attribute name="processRouteName" type="xs:string"/>
