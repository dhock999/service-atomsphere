			<xs:extension base="BusinessRuleInput">
				<xs:sequence>
					<xs:element minOccurs="0" maxOccurs="unbounded" name="inputMapping"
						type="BusinessRuleFunctionMapping">
						<xs:annotation>
							<xs:appinfo>
								<jxb:property name="mappings" />
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:any>
						<xs:annotation>
							<xs:appinfo>
								<jxb:property name="functionStep" />
							</xs:appinfo>
						</xs:annotation>
					</xs:any>
				</xs:sequence>
				<xs:attribute name="alias" type="xs:string" use="required" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
    
    <xs:complexType name="BusinessRuleFunctionMapping">
        <xs:attribute name="fromElementKey" type="xs:int" use="required"/>
        <xs:attribute name="fromTaglist" type="xs:int"/>
        <xs:attribute name="fromName" type="xs:string" use="required"/>
        <xs:attribute name="toFunctionInputKey" type="xs:int"/>
    </xs:complexType>
    
    <xs:complexType name="BusinessRuleErrorMessage">
            <xs:sequence>
           <xs:element minOccurs="0" maxOccurs="unbounded" name="input" type="BusinessRuleErrorMessageInput">
          	<xs:annotation>
            	<xs:appinfo>
                	<jxb:property name="inputs"/>
                </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="content" type="xs:string"/>
    </xs:complexType>
    
   <xs:complexType name="BusinessRuleErrorMessageInput">
        <xs:sequence>
           <xs:element minOccurs="1" maxOccurs="1" name="input" type="ConditionInput" />
        </xs:sequence>
        <xs:attribute name="index" type="xs:int" use="required"/>
    </xs:complexType>
    
    <xs:complexType name="Condition" abstract="true"/>
    
    <xs:complexType name="ConditionInput" abstract="true">
        <xs:attribute name="name" type="xs:string" use="required"/>
        <xs:attribute name="id" type="xs:int" use="required"/> 
        <xs:attribute name="deleted" type="xs:boolean" use="required" default="false"/> 
    </xs:complexType>
    
    <xs:complexType name="FunctionConditionInput">
        <xs:complexContent>
            <xs:extension base="ConditionInput">            
                <xs:attribute name="outputId" type="xs:int" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:complexType name="StaticConditionInput">
        <xs:complexContent>
            <xs:extension base="ConditionInput">
                <xs:attribute name="value" type="xs:string" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:complexType name="FieldConditionInput">
        <xs:complexContent>
            <xs:extension base="ConditionInput"/>
        </xs:complexContent>
    </xs:complexType>
    
    
    <xs:complexType name="SimpleCondition">
        <xs:complexContent>
            <xs:extension base="Condition">
                <xs:sequence>
                    <xs:element name="leftInput" type="ConditionInput" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="rightInput" type="ConditionInput" minOccurs="1" maxOccurs="1"/>
                </xs:sequence>
                <xs:attribute name="operator" type="ConditionOperator" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="GroupingCondition">
        <xs:complexContent>
            <xs:extension base="Condition">
                <xs:sequence>
                    <xs:element name="nestedExpression" type="Condition" minOccurs="0" maxOccurs="unbounded"/>                      
                </xs:sequence>
                <xs:attribute name="operator" type="GroupingConditionOperator" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:simpleType name="GroupingConditionOperator">
        <xs:annotation>
            <xs:appinfo>
                <jxb:typesafeEnumClass>
                    <jxb:typesafeEnumMember name="AND" value="and"/>
                    <jxb:typesafeEnumMember name="OR" value="or"/>
                </jxb:typesafeEnumClass>
            </xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="and"/>
            <xs:enumeration value="or"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="ConditionOperator">
        <xs:annotation>
            <xs:appinfo>
                <jxb:typesafeEnumClass>
                    <jxb:typesafeEnumMember name="EQUAL" value="="/>
                    <jxb:typesafeEnumMember name="NOT_EQUAL" value="!="/>
                    <jxb:typesafeEnumMember name="LESS_THAN" value="&lt;"/>
                    <jxb:typesafeEnumMember name="GREATER_THAN" value=">"/>
                    <jxb:typesafeEnumMember name="LESS_OR_EQUAL" value="&lt;="/>
                    <jxb:typesafeEnumMember name="GREATER_OR_EQUAL" value=">="/>
                    <jxb:typesafeEnumMember name="CONTAINS" value="contains"/>
                    <jxb:typesafeEnumMember name="NOT_CONTAINS" value="does not contain"/>
                    <jxb:typesafeEnumMember name="STARTS_WITH" value="starts with"/>
                    <jxb:typesafeEnumMember name="IS_EMPTY" value="is empty"/>
                    <jxb:typesafeEnumMember name="IS_NOT_EMPTY" value="is not empty"/>
                </jxb:typesafeEnumClass>
            </xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="="/>
            <xs:enumeration value="!="/>
            <xs:enumeration value="&lt;"/>
            <xs:enumeration value=">"/>
            <xs:enumeration value="&lt;="/>
            <xs:enumeration value=">="/>
            <xs:enumeration value="contains"/>
            <xs:enumeration value="does not contain"/>
            <xs:enumeration value="starts with"/>
            <xs:enumeration value="is empty"/>
            <xs:enumeration value="is not empty"/>
        </xs:restriction>
    </xs:simpleType>
    

    <!-- Catch Errors -->
